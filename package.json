{
  "name": "npc-cli-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "concurrently 'npm run dev-turbopack' 'npm run assets' 'npm run watch-assets'",
    "dev-turbopack": "next dev --turbopack -H 0.0.0.0",
    "dev-webpack": "next dev",
    "lint": "next lint",
    "postinstall": "patch-package",
    "start": "next start",
    "assets": "npm run ts-script scripts/assets -- --all",
    "assets-fast": "sucrase-node scripts/assets",
    "assets-bun": "bun scripts/assets --all",
    "clean-assets": "rm public/{assets.json,geomorphs.json} public/2d/{obstacles,decor}.png{,.webp}",
    "cwebp": "npm run ts-script scripts/cwebp",
    "cwebp-fast": "sucrase-node scripts/cwebp",
    "get-pngs": "npm run ts-script scripts/get-pngs",
    "get-pngs-fast": "sucrase-node scripts/get-pngs",
    "pre-push": "npm run assets-fast -- --prePush",
    "ts-script": "ts-node -r tsconfig-paths/register -O '{ \"module\": \"commonjs\", \"isolatedModules\": false, \"types\": [\"node\"] }'",
    "types": "tsc --noEmit",
    "watch-assets": "source scripts/watch-assets.sh",
    "watch-assets-nodemon": "sucrase-node scripts/assets-nodemon.js"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.14.0",
