    "earcut": "^2.2.4",
    "flexlayout-react": "^0.7.15",
    "getopts": "^2.3.0",
    "htmlparser2": "^10.0.0",
    "javascript-stringify": "^2.1.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "lodash.clonedeep": "^4.5.0",
    "maath": "^0.10.8",
